    <ClCompile>
      <AdditionalIncludeDirectories>$(OPENCV_INSTALL_DIR)\build\include;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
    </ClCompile>
    <Link>
      <AdditionalLibraryDirectories>$(OPENCV_INSTALL_DIR)\build\x64\vc11\lib%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
    </Link>
  </ItemDefinitionGroup>

</Project>